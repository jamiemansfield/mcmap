{
  "contexts": [
    {
      "id": "libraries",
      "blacklist": [
        "com/google/",
        "com/mojang/authlib/",
        "com/mojang/brigadier/",
        "com/mojang/datafixers/",
        "com/mojang/util/",
        "io/netty/",
        "it/unimi/dsi/fastutil/",
        "javax/annotation/",
        "joptsimple/",
        "net/minecraftforge/api/distmarker/",
        "org/apache/"
      ]
    }
  ],
  "default_context": "libraries",
  "mappers": [
    {
      "id": "enum_constants",
      "type": "enum_constants",
      "config": {
        "map_synthetic_values": true
      }
    },
    {
      "id": "gson_field",
      "type": "field_name",
      "config": {
        "requirements": {
          "static": true,
          "final": true,
          "synthetic": false
        },
        "desc": "Lcom/google/gson/Gson;",
        "name": "GSON"
      }
    },
    {
      "id": "logger_field",
      "type": "field_name",
      "config": {
        "requirements": {
          "static": true,
          "final": true,
          "synthetic": false
        },
        "desc": "Lorg/apache/logging/log4j/Logger;",
        "name": "LOGGER"
      }
    },
    {
      "id": "server_field",
      "type": "field_name",
      "config": {
        "requirements": {
          "static": false,
          "synthetic": false
        },
        "desc": "Lnet/minecraft/server/MinecraftServer;",
        "name": "server"
      }
    },
    {
      "id": "dispatcher_field",
      "type": "field_name",
      "config": {
        "requirements": {
          "static": false,
          "synthetic": false
        },
        "desc": "Lcom/mojang/brigadier/CommandDispatcher;",
        "name": "dispatcher"
      }
    },
    {
      "id": "profile_field",
      "type": "field_name",
      "config": {
        "requirements": {
          "static": false,
          "synthetic": false
        },
        "desc": "Lcom/mojang/authlib/GameProfile;",
        "name": "profile"
      }
    },
    {
      "id": "profile_repository_field",
      "type": "field_name",
      "config": {
        "requirements": {
          "static": false,
          "synthetic": false
        },
        "desc": "Lcom/mojang/authlib/GameProfileRepository;",
        "name": "profileRepository"
      }
    },
    {
      "id": "session_service_field",
      "type": "field_name",
      "config": {
        "requirements": {
          "static": false,
          "synthetic": false
        },
        "desc": "Lcom/mojang/authlib/minecraft/MinecraftSessionService;",
        "name": "sessionService"
      }
    }
  ]
}
